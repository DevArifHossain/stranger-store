<script type="text/mustache" id="{{DeliveryDatesOverlay}}">
<div class="as-deliverydatesoverlay clearfix large-centered">
    <div class="as-deliverydatesoverlay-heading-wrap">
        <h2 id="dudeOverlayHeading" class="as-deliverydatesoverlay-heading">{{overleyHeader}}</h2>
    </div>
    <h3 class="as-deliverydatesoverlay-subheading">{{productName}}</h3>
    <h3 class="as-deliverydatesoverlay-subheading as-deliverydatesoverlay-littleLocationLabel">{{locationLabel}}</h3>

    <form class='as-deliverydatesoverlay-addressform form-related-styles-has-to-be-remove clearfix form-table' autocomplete="off">

      {{#addressObject.fields}}
        {{#compare type '===' "select"}}
            <validator data-id="{{name}}" {{#required}} data-req="{{requiredErrorMsg}}" {{/required}} data-regex='{{invalidRegexString}}' data-ignore-events="focusout" data-show-error>

                    <label class="visuallyhidden" for="{{name}}" data-autom="deliveryDateInputField">{{label}}</label>

                    <select name="{{name}}" id="{{name}}" class="form-cell form-dropdown">
                    </select>

                    <span class="form-icon icon icon-chevrondown">&nbsp;</span>
                    <div class="ase-validator-message form-message-wrapper" id="{{name}}-error"></div>
            </validator>
        {{/compare}}
        {{#compare type '===' "text"}}
            <validator data-id="{{name}}" {{#required}} data-req="{{requiredErrorMsg}}" {{/required}} data-regex='{{invalidRegexString}}' data-ignore-events="focusout" data-show-error>

                    <label class="visuallyhidden" for="{{name}}" data-autom="deliveryDateInputField">{{label}}</label>

                    <input type="text" name="{{name}}" id="{{name}}" class="as-deliverydatesoverlay-addressField-{{name}} form-cell form-textbox form-textbox-text" {{#value}} value="{{value}}" {{/value}} {{#maxSize}} maxlength="{{maxSize}}" {{/maxSize}} placeholder= "{{label}}" data-ase-placeholder="shim"/>
                    <div class="ase-validator-message form-message-wrapper" id="{{name}}-error"></div>
            </validator>
        {{/compare}}
      {{/addressObject.fields}}

      <span class="as-deliverydatesoverlay-geospan">
        {{#compare geoEnabled '===' "true" }}
           <button type="button" class="as-geoicon">
             <span class="a11y">{{a11yGeoOnText}}</span>
           </button>
        {{/compare}}
        {{#buttonText}}
          <button class='as-deliverydatesoverlay-addressform-button button merchandising' type="submit" data-autom="viewOptionsButton">{{buttonText}}</button>
          <span class="as-viewoptions-button-msg visuallyhidden" aria-live="polite"></span>
        {{/buttonText}}
      </span>
      <div class="as-geoerror">
      </div>
      <div class="ase-delivery-notice"></div>

    </form>

    <div class="as-deliverydatesoverlay-overlaydatestable">
    </div>
    <div class="as-deliverydatesoverlay-geonotice">{{geoNotice}}</div>
</div>

</script>
<script type="text/mustache" id="{{CommitCodeTable}}">
<ul class="as-datatable-container">
	{{#deliveryOptions}}
		<li class="as-datatable-row as-deliverydatesoverlay-deliveryoptions-list" data-autom="deliveryDateOptions">
			<span class="as-datatable-cell" data-autom="deliveryQuote"> {{displayName}} </span>
			<span class="as-datatable-cell as-deliverydatesoverlay-deliveryoptions-date" data-autom="deliveryDate">  {{date}} </span>
			<span class="as-datatable-cell as-deliverydatesoverlay-deliveryoptions-shippingcost" data-autom="deliveryCost">  {{shippingCost}} </span>
		</li>
	{{/deliveryOptions}}
</ul>
</script>

        <script type="text/mustache" id="{{pincode}}">
<div class="rs-buyflow-pincodecontent {{^displayHalf}} as-l-container {{/displayHalf}} {{#displayHalf}} as-l-container-full-small{{/displayHalf}}" role="group" aria-labelledby="as-pincode-header">
    <materializer data-shown="{showPincodeDefault}">
        <div class="row">
            <div class="as-pincode-headerwrapper column {{#displayHalf}} large-12 {{/displayHalf}} {{^displayHalf}} large-8 {{/displayHalf}} small-12">
                <div id="as-pincode-header" class="as-pincode-header">{{pincodeDeliveryHeader}}</div>
                <span>
                    {{pincodeDeliveryMessage}}
                </span>
            </div>
            <div class="as-pincode-controlwrapper column {{#displayHalf}} large-12 {{/displayHalf}} {{^displayHalf}} large-4 {{/displayHalf}} small-12">
                <form class='as-pincode-form clearfix form-table' autocomplete="off">
                {{#pinCode}}
                    <validator data-id="postalCode" {{#required}} data-req="{{requiredErrorMsg}}" {{/required}} data-show-error="true" data-regex=' {"{{validRegexes}}":"{{invalidErrorMsg}}"} '>
                        <label class="visuallyhidden" for="postalCode">{{label}}</label>
                        <input type="text" name="postalCode" id="postalCode" class="form-cell form-textbox form-textbox-text" {{#maxSize}} maxlength="{{maxSize}}" {{/maxSize}} placeholder="{{label}}" data-ase-placeholder="shim" />
                        <div class="form-icons-wrapper form-icons-focusable">
                            <div data-render="input-reset" data-target="#postalCode" data-clear-text="Reset" class="form-icons form-icons-small"></div>
                        </div>
                        <div class="ase-validator-message form-message-wrapper" id="postalCode-error"></div>
                        <span class="as-pincode-verifybtn">
                            {{#buttonText}}
                            <button class='button merchandising' type="submit">{{buttonText}}</button>
                            {{/buttonText}}
                        </span>
                    </validator>
                {{/pinCode}}
                </form>
            </div>
        </div>
    </materializer>
    <materializer data-shown="{showDeliveryMessage}">
        <div>
            <span class="as-pincode-success"></span>
            <a class="as-pincode-default" href="#"></a>
        </div>
    </materializer>
</div>

</script>

